#: usr/share/mauna/maunawelcome/maunawelcome.ui.h:1
msgid "Welcome"
msgstr "Mirë se vini"

#: usr/lib/mauna/maunawelcome/maunawelcome.py:134
msgid "First Steps"
msgstr "Hapat e parë"

#: usr/lib/mauna/maunawelcome/maunawelcome.py:139
#: usr/share/mauna/maunawelcome/maunawelcome.ui.h:5
msgid "Documentation"
msgstr "Dokumentacioni"

#: usr/lib/mauna/maunawelcome/maunawelcome.py:143
msgid "Help"
msgstr "Ndihmë"

#: usr/lib/mauna/maunawelcome/maunawelcome.py:147
#: usr/share/mauna/maunawelcome/maunawelcome.ui.h:2
msgid "Contribute"
msgstr "Kontribuoni"

#: usr/lib/mauna/maunawelcome/maunawelcome.py:154
